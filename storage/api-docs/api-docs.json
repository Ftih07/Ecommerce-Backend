            "delete": {
                "tags": [
                    "Order"
                ],
                "summary": "Delete specific order",
                "operationId": "303a7b797887b66c7790c4c187b54790",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Order deleted"
                    },
                    "404": {
                        "description": "Order not found"
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "Order",
            "description": "Order"
        }
    ]
